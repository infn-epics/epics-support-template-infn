# Decreases the resolution of both delay and width by an integer multiple.
#
# Achieves greater range at the expense of resolution.
#
record(longout, "$(P)DlyGen0Prescaler-SP") {
  field(DTYP, "Obj Prop uint32")
  field(OUT , "@OBJ=$(EVG):EVR$(T):Pul0, PROP=Prescaler")
  field(DESC, "Pulser prescaler")
  field(PINI, "YES")
  field(HOPR, "0xffff")
  field(LOPR, "1")
  field(DRVH, "0xffff")
  field(DRVL, "1")
  field(VAL , "1")
  field(FLNK, "$(P)DlyGen0Prescaler-RB")
  field(DISP, "0")
  field(DISA, "0")
  info(autosaveFields_pass0, "VAL")
}

record(longin, "$(P)DlyGen0Prescaler-RB") {
  field(DTYP, "Obj Prop uint32")
  field(INP , "@OBJ=$(EVG):EVR$(T):Pul0, PROP=Prescaler")
  field(HOPR, "0xffff")
  field(LOPR, "1")
  field(HIGH, "0xffff")
  field(HSV, "MAJOR")
  field(FLNK, "$(P)DlyGen0Res-I")
}

record(calc, "$(P)DlyGen0Res-I") {
  field(DESC, "Effective resolution")
  field(INPA, "$(P)DlyGen0Prescaler-RB MS")
  field(INPB, "$(P)LinkClkPeriod-I CP")
  field(CALC, "1e9*(A?A:1)*B")
  field(EGU, "ns")
  field(FLNK, "$(P)DlyGen0Res-FO_")
}

