  field(DTYP, "Obj Prop bool")
  field(DESC, "Output RTM (6)")
  field(OUT , "@OBJ=$(EVR):RearUniv6, PROP=Enable")
  field(PINI, "YES")
  field(ZNAM, "Disabled")
  field(ONAM, "Enabled")
  field(VAL , "1") # Default to enabled
  info(autosaveFields_pass0, "VAL")
}

