  field(PINI, "YES")
  field(ZRST, "Event clock")
  field(ONST, "Mapped code(s)")
  field(TWST, "DBus 4")
  field(ZRVL, "0")
  field(ONVL, "1")
  field(TWVL, "2")
  field(THSV, "INVALID")
  field(FRSV, "INVALID")
